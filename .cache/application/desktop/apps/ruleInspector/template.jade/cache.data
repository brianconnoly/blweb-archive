<div class="dragHandler topBar noMaximize noSettings black flushMousedown"><process-bar></process-bar><div class="appControls"></div><div translate="'ruleInspectorApp_title'" class="title"></div></div><div class="body dragHandler flushMousedown"><div ng-show="status.multiEdit" class="multiEditPopup"><div translate="'ruleInspectorApp_selection'" class="blockTitle"></div><div class="line selectedCounter"><div translate="'inspector_selected_count'" class="capture"></div><div class="value">{{status.selectedPlaceholders.length}}</div></div><div translate="'ruleInspectorApp_repeatTitle'" class="blockTitle"></div><div class="line"><div translate="'inspector_repeat_type'" class="capture"></div><div class="value"><div ng-click="setMultiPlan('single')" ng-class="{'cur':status.multiType=='single'}" title="{{localization.translate('inspector_single')}}" class="repeatIcon flushMousedown single"><div></div></div><div ng-click="setMultiPlan('daily')" ng-class="{'cur':status.multiType=='daily'}" title="{{localization.translate('inspector_daily')}}" class="repeatIcon flushMousedown daily"><div></div></div></div></div><div ng-switch="status.multiType" class="desctiprionBlock"><div ng-switch-when="single" translate="'placeholderSingle_description'" class="text"></div><div ng-switch-when="daily" translate="'placeholderCycle_description'" class="text"></div></div><div ng-show="status.multiType"><div ng-show="status.multiType=='daily'" class="line"><div class="capture">{{localization.translate('inspector_days_repeat')}}</div><div class="value daySelector"><div ng-repeat="day in daysArray" ng-click="selectDay($event, day)" ng-class="getDayClass(day)" class="day">{{daysValues[day]}}</div></div></div><div ng-show="status.multiType!=''" class="line"><div translate="'inspector_ad'" class="capture wideCapture"></div><div class="value v_advert"><checkbox ng-model="status.multiAd" ng-change="saveMulti()" class="long"></checkbox></div></div><div ng-show="status.multiType!='single'" class="line"><div translate="'inspector_ending'" class="capture"></div><div class="value v_advert"><checkbox ng-model="status.multiEnd" ng-change="saveMulti()" class="long"></checkbox></div></div><div ng-show="status.multiType!='single' &amp;&amp; status.multiEnd" class="line"><div class="capture"></div><div class="value"><div ng-model="status.multiEndDay" dateonly="true" options="inspector" ng-change="saveMulti()" class="date datePicker">{{status.multiEndDay | timestampMask: "DD MMM YYYY"}}</div></div></div></div><div ng-click="setFocusRule()" ng-show="selected.length==1" class="line focusRule"><div style="width:200px" class="capture wideCapture selectDay">Выбрать правило</div></div></div><div ng-show="!status.multiEdit" class="singleEditPopup"><div translate="'ruleInspectorApp_lengthTitle'" class="blockTitle"></div><div class="line"><div ng-dblclick="scrollTo(startDay)" translate="'inspector_timestamp_start'" class="capture"> </div><div class="value"><div ng-model="status.currentRule.timestampStart" dateonly="true" options="inspector" ng-change="saveRule()" class="date datePicker">{{status.currentRule.timestampStart | timestampMask: "DD MMM YYYY"}}</div><div ng-model="status.currentRule.timestampStart" ng-change="saveRule()" min="5" class="timePicker timePickerInspecor"></div></div></div><div ng-class="{'hidden':status.currentRule.ruleType=='single'}" class="line"><div translate="'inspector_ending'" class="capture"></div><div class="value v_advert"><checkbox ng-model="status.currentRule.end" ng-change="saveRule()" class="long"></checkbox></div></div><div ng-class="{'hidden':status.currentRule.ruleType=='single' || !status.currentRule.end}" class="line"><div class="capture"></div><div class="value"><div ng-model="status.currentRule.timestampEnd" dateonly="true" options="inspector" edgetime="status.currentRule.timestampStart" ng-change="saveRule()" class="date datePicker">{{status.currentRule.timestampEnd | timestampMask: "DD MMM YYYY"}}</div><div ng-model="status.currentRule.timestampEnd" ng-show="status.currentRule.ruleType!='daily'" ng-change="saveRule()" class="timePicker timePickerInspecor"></div></div></div><div translate="'ruleInspectorApp_repeatTitle'" class="blockTitle"></div><div class="line"><div translate="'inspector_repeat_type'" class="capture"></div><div class="value"><div ng-click="setPlan('single')" ng-class="{'cur':status.currentRule.ruleType=='single'}" title="{{localization.translate('inspector_single')}}" class="repeatIcon single"><div></div></div><div ng-click="setPlan('daily')" ng-class="{'cur':status.currentRule.ruleType=='daily'}" title="{{localization.translate('inspector_daily')}}" class="repeatIcon daily"><div></div></div><div ng-click="setPlan('chain')" ng-class="{'cur':status.currentRule.ruleType=='chain'}" title="{{localization.translate('inspector_chain')}}" class="repeatIcon chain"><div></div></div></div></div><div ng-switch="status.currentRule.ruleType" class="desctiprionBlock"><div ng-switch-when="single" translate="'placeholderSingle_description'" class="text"></div><div ng-switch-when="daily" translate="'placeholderCycle_description'" class="text"></div><div ng-switch-when="chain" translate="'placeholderSeries_description'" class="text"></div></div><div ng-switch="status.currentRule.ruleType" class="settingsBlock"><div ng-switch-when="chain" class="line"><div translate="'inspector_interval'" class="capture"></div><div class="value"><div ng-model="status.currentRule.interval" options="inspectorInterval" ng-change="saveRule()" min="5" class="timePicker timePickerInspecor"></div></div></div><div ng-switch-when="daily" class="line"><div class="capture">{{localization.translate('inspector_days_repeat')}}</div><div class="value daySelector"><div ng-repeat="day in daysArray" ng-click="selectDay($event, day)" ng-class="getDayClass(day)" class="day">{{daysValues[day]}}</div></div></div></div><div ng-class="{'hidden':status.currentRule.ruleType=='chain'}" class="line"><div translate="'inspector_ad'" class="capture wideCapture"></div><div class="value v_advert"><checkbox ng-model="status.currentRule.ad" ng-change="saveRule()" class="long"></checkbox></div></div><div ng-click="focusDay()" class="line focusDay"><div class="capture">{{localization.translate('select_day')}}</div></div><div ng-click="removeAll()" class="remove">{{localization.translate('inspector_removeAll')}}</div></div></div>