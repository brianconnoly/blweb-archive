.contactsPage .mainBlock {
  width: 80%;
  display: inline-block;
  margin: 0 auto;
}
.contactsPage .mainBlock .column {
  width: 100%;
  padding: 0 10px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  text-align: center;
  font-size: 13px;
  height: 100px;
}
.contactsPage .mainBlock .column .cell {
  padding-bottom: 4px;
}
.contactsPage .mainBlock .column2 {
  width: 50%;
  float: left;
  padding: 0 10px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  text-align: left;
  font-size: 14px;
  height: 150px;
}
.contactsPage .mainBlock .column2 .cell {
  padding-bottom: 14px;
}
.contactsPage .mainBlock .column3 {
  width: 33%;
  float: left;
  padding: 0 10px;
  -webkit-box-sizing: border-box;
  -moz-box-sizing: border-box;
  box-sizing: border-box;
  font-size: 13px;
}
.contactsPage .mainBlock .column3 .imgClocks {
  background-image: url("/resources/images/icons/paymentinfo/contactsPage/freakyclocks.png");
  background-position: center center;
  background-repeat: no-repeat;
  -webkit-background-size: 120px 90px;
  -moz-background-size: 120px 90px;
  background-size: 120px 90px;
  width: 100%;
  height: 90px;
}
