<div class="topBar dragHandler"><div class="appControls"></div><div class="title">Svg tunner</div><div class="switcherPanel"><input ng-model="size.width"/><input ng-model="size.height"/><input ng-model="size.zoom"/><input ng-model="path"/><checkbox ng-model="gridOnTop" class="long"></checkbox></div></div><div ng-class="{'gridOnTop':gridOnTop}" class="body"><div ng-style="{'width':size.width * size.zoom,'height':size.height * size.zoom}" class="svgTunnerGrid"></div><img ng-src="{{path}}" ng-style="{'width':size.width * size.zoom,'height':size.height * size.zoom}" class="svgPreview"/></div>