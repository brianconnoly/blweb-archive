<div class="dragHandler topBar black noMaximize noSettings"><div class="appControls"></div><div class="title">Moderation</div></div><div class="body"><div ng-repeat="item in moderationLots" class="moderationLot"><div class="controlBlock"><combobox list="lotCategories" ng-model="item.lotCat"></combobox><button ng-click="acceptLot(item)">{{localization.translate('moderation_accept_lot')}}</button><button ng-click="rejectLot(item)">{{localization.translate('moderation_reject_lot')}}</button></div><div class="lotPreview"></div><div ng-show="item.tags.length&gt;0" class="tagList"><div ng-repeat="tag in item.tags" class="tag">{{tag}}</div></div></div></div>