<div class="dragHandler topBar noSettings black"><process-bar></process-bar><div class="appControls"></div><div ng-click="stepBack()" ng-if="canGoBack()" class="backButton"></div><div ng-if="session.item.type == 'schedule'" translate="'graphApp_title_sched'" class="title"></div><div ng-if="session.item.type == 'statsCut'" ng-switch="item.statsCutType" class="title"><span ng-switch-when="day">{{::(item.timestamp - DAY) | timestampMask:'DD ofMMMM, dddd'}}</span><span ng-switch-when="week"> <span translate="'graphApp_title_week_from'"></span><span> {{::(item.timestamp - WEEK) | timestampMask:'DD ofMMMM'}} </span><span translate="'graphApp_title_week_till'"></span><span> {{::item.timestamp - DAY | timestampMask:'DD ofMMMM'}}</span></span><span ng-switch-when="month" class="month">{{::(item.timestamp - MONTH - DAY - DAY) | timestampMask:'MMMM'}} </span></div></div><div class="body"><div class="uniGraph"><div class="typeSwitch"><div ng-class="{'active':graphType == 'cumulative'}" translate="'graphApp_type_cumulative'" ng-click="setGraphType('cumulative')" class="button"></div><div ng-class="{'active':graphType == 'dynamic'}" translate="'graphApp_type_dynamic'" ng-click="setGraphType('dynamic')" class="button"></div></div><div translate="'graphApp_description_' + graphType + '_' + linesType" class="graphDescription"></div><div class="graphContainer"><div class="hoverBar"></div><canvas class="fullSize"></canvas><div class="dots fullSize"></div><div class="helper"></div></div><div class="legend"><div ng-repeat="line in lines" ng-click="toggleLine(line)" class="line"><span ng-class="line.type" class="icon"></span><span class="number">{{line.total || 0}}</span><div ng-style="{'background-color':line.color}" ng-if="lines.length &gt; 1 &amp;&amp; showLine[line.type]" class="color"></div></div></div><div class="overallSwitch"><div translate="'graphApp_overall_' + linesType" ng-click="toggleLinesType()" class="button"></div></div></div></div>