.appContainer.searchMedia {
  background: #75a843;
  color: #494949;
}
.appContainer.searchMedia .scroll-bottom,
.appContainer.searchMedia .scroll-right {
  background: rgba(117,168,67,0.8) !important;
}
.appContainer.searchMedia .body {
  overflow-y: auto;
}
.appContainer.searchMedia .body .stepView {
  top: 50px;
}
.appContainer.searchMedia .paginator .page {
  border-color: #75a843;
}
.appContainer.searchMedia .paginator .page.active {
  background: #75a843;
}
.appContainer.searchMedia .searchPanel {
  overflow: hidden;
  width: 100%;
  padding-bottom: 10px;
}
.appContainer.searchMedia .searchPanel .searchBox {
  margin: 13px auto;
  width: 225px;
  height: 23px;
  border: 1px solid #494949;
  -webkit-border-radius: 3px;
  border-radius: 3px;
  -webkit-transition: width 0.2s ease;
  -moz-transition: width 0.2s ease;
  -o-transition: width 0.2s ease;
  -ms-transition: width 0.2s ease;
  transition: width 0.2s ease;
}
.appContainer.searchMedia .searchPanel .searchBox.long {
  width: 400px;
}
.appContainer.searchMedia .searchPanel .searchBox .inputContainer {
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  right: 45px;
  border-right: 1px solid #494949;
}
.appContainer.searchMedia .searchPanel .searchBox .inputContainer input {
  width: 100%;
  height: 100%;
  padding: 0 8px;
  box-modex: border-box;
  font-size: 12px;
  line-height: 23px;
  color: #494949;
}
.appContainer.searchMedia .searchPanel .searchBox .inputContainer input::-webkit-input-placeholder {
  color: #999;
}
.appContainer.searchMedia .searchPanel .searchBox .inputContainer input::-moz-input-placeholder {
  color: #999;
}
.appContainer.searchMedia .searchPanel .searchBox .button {
  position: absolute;
  right: 0;
  bottom: 0;
  top: 0;
  width: 44px;
  text-align: center;
  font-size: 12px;
  line-height: 23px;
  cursor: pointer;
}
.appContainer.searchMedia .searchPanel .helper {
  position: absolute;
  left: 0;
  right: 0;
  bottom: 5px;
  text-align: center;
  font-size: 10px;
  color: #999;
  opacity: 0;
  -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
  filter: alpha(opacity=0);
  -webkit-transition: opacity 0.2s ease;
  -moz-transition: opacity 0.2s ease;
  -o-transition: opacity 0.2s ease;
  -ms-transition: opacity 0.2s ease;
  transition: opacity 0.2s ease;
}
.appContainer.searchMedia .searchPanel .helper.active {
  opacity: 1;
  -ms-filter: none;
  filter: none;
}
.appContainer.searchMedia .importActions {
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  height: 60px;
}
