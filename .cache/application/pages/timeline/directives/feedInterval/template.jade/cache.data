<div timeline-dropable="timeline-dropable" item="getTimeItem()" ng-dblclick="intervalDblClick($event)" class="contextMenu contents liveInterval droppableItem"><div ng-if="startDay" class="dayStat"></div><div dropable-item="sched" ng-repeat="sched in schedular" ng-class="'sched_'+sched.id" type="post" class="contextMenu feedItem selectableItem ios-nodelay droppableItem editableItem tl_feeditem"><div type="post" id="sched.postId" overlay="timeline" ng-if="sched.type=='schedule'" class="itemPreview"></div><div ng-if="sched.status == 'error'" ng-click="sendNow($event)" title="Отправить сейчас" class="sendNowButton flushMousedown"></div><div ng-if="sched.stats.length &gt; 0" ng-click="showGraph($event)" title="Хронология" class="watchStatButton flushMousedown"></div></div></div>