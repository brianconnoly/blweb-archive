tC['/novaVideoPreview/template.jade'] = "<img src=\"{{item.thumbnail}}\" class=\"picPreload\"/> <div class=\"bottomShield\"> <div class=\"topLine name\"> <div class=\"wrapper\"> <div class=\"centered\"><img src=\"/resources/images/nova/entities/play.svg\"/><span>{{::item.duration | secondsToHumanTime}}</span><span> ({{getSource(item)}})</span></div> </div> </div> <div class=\"bottomLine\"> <div class=\"inner name\">{{item.title}}</div> </div> </div>";

registerStyles('novaVideoPreview', ".novaVideoPreview {\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  overflow: hidden;\n}\n.novaVideoPreview img {\n  pointer-events: none;\n}\n.novaVideoPreview .bottomShield {\n  position: absolute;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  width: 100%;\n  height: 51%;\n  min-height: 25px;\n  background: rgba(0,0,0,0.7);\n  color: #fff;\n  opacity: 1;\n  -ms-filter: none;\n  filter: none;\n  -webkit-transition: opacity 0.2s ease;\n  -moz-transition: opacity 0.2s ease;\n  -o-transition: opacity 0.2s ease;\n  -ms-transition: opacity 0.2s ease;\n  transition: opacity 0.2s ease;\n}\n.noItemNames .novaVideoPreview .bottomShield {\n  opacity: 0;\n  -ms-filter: \"progid:DXImageTransform.Microsoft.Alpha(Opacity=0)\";\n  filter: alpha(opacity=0);\n}\n.novaVideoPreview .bottomShield .topLine {\n  height: 33%;\n  text-align: center;\n}\n.novaVideoPreview .bottomShield .topLine .wrapper {\n  position: absolute;\n  bottom: 0;\n  width: 100%;\n}\n.novaVideoPreview .bottomShield .topLine .centered {\n  display: inline-block;\n}\n.novaVideoPreview .bottomShield .topLine .centered img {\n  vertical-align: bottom;\n  height: 1em;\n}\n.novaVideoPreview .bottomShield .bottomLine {\n  margin: 0 5%;\n  height: 67%;\n  width: 90%;\n  display: table;\n}\n.novaVideoPreview .bottomShield .inner {\n  display: table-cell;\n  vertical-align: middle;\n  text-align: center;\n  color: #fff;\n  font-size: 10px;\n}\n");
