div.combobox-list {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(0,0,0,0.3);
  z-index: 100500;
}
div.combobox-list .content {
  position: absolute;
  min-width: 30px;
  min-height: 36px;
  background: rgba(255,255,255,0.9);
  border-top: 1px solid #ddd;
  overflow: auto;
}
div.combobox-list .list {
  display: inline-block;
  min-width: 100%;
}
div.combobox-list .list li {
  height: 36px;
  line-height: 36px;
  padding-left: 10px;
  padding-right: 36px;
  white-space: nowrap;
}
div.combobox-list .list li.selected {
  background-image: url("/resources/images/icons/markers/checked.png");
  background-position: right top;
  background-repeat: no-repeat;
  -webkit-background-size: 36px 36px;
  -moz-background-size: 36px 36px;
  background-size: 36px 36px;
}
@media all and (-webkit-min-device-pixel-ratio: 1.5), (min--moz-device-pixel-ratio: 1.5), (-o-min-device-pixel-ratio: 1.5/1), (min-device-pixel-ratio: 1.5), (min-resolution: 138dpi), (min-resolution: 1.5dppx) {
  div.combobox-list .list li.selected {
    background-image: url("/resources/images/icons/markers/checked@2x.png");
    background-position: right top;
    background-repeat: no-repeat;
    -webkit-background-size: 36px 36px;
    -moz-background-size: 36px 36px;
    background-size: 36px 36px;
  }
}
div.combobox-list .list li:hover {
  background-color: #fff;
  cursor: pointer;
}
div.combobox-list.hideSelected .selected {
  display: none;
}
div.combobox-list.snIcon .list {
  border: 0;
}
div.combobox-list.snIcon .list li {
  padding-left: 0;
  padding-right: 0;
  height: 40px;
  line-height: 39px;
  font-size: 18px;
  color: #fff;
  text-transform: uppercase;
  text-align: center;
}
