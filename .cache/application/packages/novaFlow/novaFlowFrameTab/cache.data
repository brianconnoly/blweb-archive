tC['/novaFlowFrameTab/template.jade'] = "<div ng-class=\"{'pinned':flowFrame.pinned, 'active':flowFrame.active &amp;&amp; flowBox.active}\" ng-click=\"activateTab()\"> <div class=\"novaFlowFrameTabBg\"></div> <div class=\"novaFlowFrameTabContent\"> <div ng-click=\"pinTab()\" class=\"tabIcon pin\"></div> <div ng-click=\"closeTab()\" class=\"tabIcon close\"></div> <div ng-click=\"closeTab()\" class=\"tabIcon closePinned\"></div> <div ng-if=\"flowFrame.item.id &amp;&amp; flowFrame.item.id != appItem.id\" class=\"novaAppItem\"></div> <div ng-if=\"!flowFrame.item.id || flowFrame.item.id == appItem.id\" class=\"frameTitle\"> <div ng-if=\"flowFrame.title &amp;&amp; !flowFrame.translateTitle\" class=\"titleText realText\">{{flowFrame.title}}</div> <div ng-if=\"flowFrame.translateTitle\" translate=\"flowFrame.translateTitle\" class=\"titleText translateText\"></div> </div> </div> </div>";

buzzlike.directive('novaFlowFrameTab', function() {
  return {
    restrict: 'C',
    template: tC['/novaFlowFrameTab/template.jade'],
    replace: true,
    link: function(scope, element, attrs, ngModel) {
      var elem;
      elem = $(element);
      scope.pinTab = function() {
        scope.flowBox.pin(scope.flowFrame);
        return true;
      };
      scope.closeTab = function() {
        return scope.flowBox.closeFlowFrame(scope.flowFrame);
      };
      return scope.activateTab = function() {
        return scope.flow.activate(scope.flowFrame, scope.flowBox);
      };
    }
  };
});

