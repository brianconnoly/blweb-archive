tC['/novaImagePreview/template.jade'] = "<img src=\"{{item.thumbnail}}\" class=\"picPreload\"/> <div ng-if=\"item.name\" class=\"name\"> <div class=\"inner\">{{item.name}}</div> </div>";

registerStyles('novaImagePreview', ".novaImagePreview {\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  overflow: hidden;\n}\n.novaImagePreview img {\n  pointer-events: none;\n}\n.novaImagePreview .name {\n  position: absolute;\n  display: table;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  width: 100%;\n  height: 33%;\n  min-height: 25px;\n  font-size: 10px;\n  background: rgba(0,0,0,0.7);\n  opacity: 1;\n  -ms-filter: none;\n  filter: none;\n  -webkit-transition: opacity 0.2s ease;\n  -moz-transition: opacity 0.2s ease;\n  -o-transition: opacity 0.2s ease;\n  -ms-transition: opacity 0.2s ease;\n  transition: opacity 0.2s ease;\n}\n.noItemNames .novaImagePreview .name {\n  opacity: 0;\n  -ms-filter: \"progid:DXImageTransform.Microsoft.Alpha(Opacity=0)\";\n  filter: alpha(opacity=0);\n}\n.novaImagePreview .name .inner {\n  display: table-cell;\n  vertical-align: middle;\n  text-align: center;\n  color: #fff;\n}\n");
