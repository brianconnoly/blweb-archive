<div class="dragHandler topBar noMaximize noSettings black"><process-bar></process-bar><div class="appControls"></div><div translate="'lotPreviewApp_title'" class="title"></div></div><div class="body dragHandler"><div class="column leftPart zoom_max"><div class="itemsContainer bigPreview"><div class="previewContainer"><div type="content" id="bigItem" class="itemPreview editableItem"></div></div></div><div ng-if="contentIds.length &gt; 0" class="itemsContainer bigPreview"><div class="previewContainer"><div class="itemPreview post"><div ng-repeat="id in contentIds" type="content" id="id" class="itemPreview mini editableItem"></div></div></div></div><div ng-click="takeToRight()" class="doRepostButton"><span translate="'lotPreviewApp_doRepostButton'" ng-if="lot.lotType == 'repost'"></span><span> {{getLotPrice('lotPreviewApp_buy_for')}}</span></div></div><div ng-if="lot.lotType == 'repost'" class="column rightPart"><div translate="'lotPreviewApp_repostParamsTitle'" class="blockTitle"></div><div class="list"><div class="line"><div translate="'lotPreviewApp_lot_title'" class="title"></div><div title="{{community.name}}" class="value">{{community.name}}</div></div><div class="line"><div translate="'lotPreviewApp_lot_communitySize'" class="title"></div><div class="value">{{community.membersCount}} {{localization.declensionPhrase(community.membersCount,'subscribers')}}</div></div><div ng-if="lot.minSubscribers &gt; 0" class="line"><div translate="'lotPreviewApp_lot_subscribers'" class="title"></div><div class="value">{{translate('requestmanager_from')}} {{lot.minSubscribers}} {{localization.declensionPhrase(lot.minSubscribers,'subscribers_from')}}</div></div><div class="line"><div translate="'lotPreviewApp_lot_timeLimit'" class="title"></div><div class="value">{{lot.timeFrom | timestampMask: "hh:mm"}} — {{lot.timeTo | timestampMask: "hh:mm"}}</div></div><div class="line"><div translate="'lotPreviewApp_lot_dateLimit'" class="title"></div><div class="value">{{lot.dateFrom | timestampMask: "DD MMM"}} — {{lot.dateTo | timestampMask: "DD MMM"}}</div></div><div ng-if="lot.checkAfter &gt; 0" class="line"><div translate="'lotPreviewApp_lot_garant'" class="title"></div><div class="value">{{getCheckAfter(lot.checkAfter)}}</div></div><div class="line"><div translate="'lotPreviewApp_lot_lotOwner'" class="title"></div><div class="value">{{owner.name}}</div></div></div><div translate="'lotPreviewApp_repostRulesTitle'" class="blockTitle"></div><div translate="'lotPreviewApp_repostRules'" class="smallText"></div></div><div ng-if="lot.lotType == 'post'" class="column rightPart"><div translate="'lotPreviewApp_postParamsTitle'" class="blockTitle"></div><div class="list"><div class="line"><div translate="'lotPreviewApp_lot_postOwner'" class="title"></div><div ng-click="goOwnerPage()" class="value ownerLink">{{owner.firstName}} {{owner.lastName}}</div></div><div class="line"><div translate="'lotPreviewApp_lot_price'" class="title"></div><div class="value">{{lot.price}} {{localization.declensionPhrase(lot.price, 'roubles')}}</div></div><div class="line"><div translate="'lotPreviewApp_lot_bought'" class="title"></div><div class="value">{{getLotCustomers()}}</div></div></div><div translate="'lotPreviewApp_postRulesTitle'" class="blockTitle"></div><div translate="'lotPreviewApp_postRules'" class="smallText"></div></div></div>