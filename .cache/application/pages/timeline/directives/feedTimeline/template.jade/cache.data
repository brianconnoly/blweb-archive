<li ng-class="{'faded': (status == -1)}" class="feedTimeline"><div stripe-color="stripe-color" title="{{item.name}}" ng-mouseenter="hideTimebar($event)" ng-mouseleave="showTimebar($event)" ng-dblclick="openCommunity()" class="contextMenu head"><div ng-model="group.startDay" jump="jumpScroll()" class="noDigest showCalendar datePicker ios-nodelay tl_opencalendar"></div><div class="photoContainer feedSort"><div ng-if="!item.photo" class="socialNetwork">{{::item.socialNetwork}}</div><img ng-src="{{::proxyImage(item.photo)}}" class="picPreload picture tl_dragtimeline"/><div style="background:{{::snColor(item.socialNetwork)}}" class="photoOverlay"><div ng-click="showFeedMenu($event)" class="menuTrigger"></div><div class="sn">{{::item.socialNetwork}}</div></div></div><div ng-model="group.startDay" jump="jumpScroll()" translate="'timelineApp_calendar'" style="border-bottom: 1px solid {{::snColor(item.socialNetwork)}}" class="headButton datePicker"></div><switcher ng-model="group.glued" class="headButton glue ios-nodelay"><span option="0" translate="'timelineApp_glue0'"></span><span option="1" translate="'timelineApp_glue1'"></span><span option="2" translate="'timelineApp_glue2'"></span></switcher>
</div><ul class="intervals tl_intervaltime"></ul><div class="timebar"><div class="time"></div></div><div class="touchBar"><div class="time"></div><div ng-click="pickPost()" class="picker ios-nodelay">+</div><div ng-click="stepDown()" class="leftArrow ios-nodelay">-5</div><div ng-click="stepUp()" class="rightArrow ios-nodelay">+5</div></div></li>