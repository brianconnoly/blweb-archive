.logBar
    height 30px
    background: rgb(80,80,80)
    color: #fff
    
    .time
        float: right
        height 30px
        
    .marker
        height 30px
        float: left
        font-weight: bolder
        line-height 30px
        width 40px
        text-align: center
        background: rgba(255, 255, 255, .2)
        
    .url
        height 30px
        line-height 30px
        font-size: 14px
        padding 0 10px
        
        text-overflow: ellipsis
        overflow: hidden
        white-space nowrap 
        
        user-select text
        cursor: text
        
.response
    white-space pre-wrap
    padding: 10px
    background: rgb(240,240,240)
    color: #000
    line-height: 16px
    font-size: 14px
    user-select text
    cursor: text
        