tC['/novaUserPreview/template.jade'] = "<div class=\"avatarWrapper\"> <div class=\"userPic\"><img src=\"{{::item.photo}}\" class=\"picPreload\"/></div> <div class=\"overlay\"></div> </div> <div ng-if=\"item.name\" class=\"userName\">{{::item.name}}</div> <div ng-if=\"!item.name\" class=\"userName\">{{::getUserIdent(item)}}</div>";

registerStyles('novaUserPreview', ".novaUserPreview .avatarWrapper {\n  width: 60px;\n  height: 60px;\n  -webkit-border-radius: 40px;\n  border-radius: 40px;\n  padding: 2px;\n  background: #fff;\n  -webkit-box-shadow: 0 2px 10px -2px rgba(0,0,0,0.3);\n  box-shadow: 0 2px 10px -2px rgba(0,0,0,0.3);\n}\n.novaUserPreview .avatarWrapper .userPic {\n  -webkit-border-radius: 50px;\n  border-radius: 50px;\n  position: absolute;\n  top: 3px;\n  bottom: 3px;\n  left: 3px;\n  right: 3px;\n  -webkit-box-sizing: border-box;\n  -moz-box-sizing: border-box;\n  box-sizing: border-box;\n  overflow: hidden;\n  -webkit-transform: translate3d(0, 0, 0);\n  -moz-transform: translate3d(0, 0, 0);\n  -o-transform: translate3d(0, 0, 0);\n  -ms-transform: translate3d(0, 0, 0);\n  transform: translate3d(0, 0, 0);\n  pointer-events: none;\n}\n.novaUserPreview .avatarWrapper .overlay {\n  position: absolute;\n  pointer-events: none;\n  top: 0;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  -webkit-border-radius: 50px;\n  border-radius: 50px;\n}\n.novaUserPreview .userName {\n  margin-top: 10px;\n  text-align: center;\n  font-size: 12px;\n  width: 64px;\n}\n");
