<div class="dragHandler topBar noMaximize noSettings black"><div class="appControls"></div><div translate="'calendarApp_title'" class="title"></div></div><div class="body dragHandler"><table><tr class="head"><td colspan="7" class="center"><div ng-click="goLeft()" class="arrow flushMousedown"><div class="button left"></div></div><div class="month">{{localization.translate(154 + current.month)[0]}} </div><div ng-click="goRight()" class="arrow flushMousedown"><div class="button right"></div></div></td></tr><tr class="week"><td class="day dayname">{{localization.translate(148)[1].toUpperCase()}}</td><td class="day dayname">{{localization.translate(149)[1].toUpperCase()}}</td><td class="day dayname">{{localization.translate(150)[1].toUpperCase()}}</td><td class="day dayname">{{localization.translate(151)[1].toUpperCase()}}</td><td class="day dayname">{{localization.translate(152)[1].toUpperCase()}}</td><td class="day dayname">{{localization.translate(153)[1].toUpperCase()}}</td><td class="day dayname">{{localization.translate(147)[1].toUpperCase()}}</td></tr><tr ng-repeat="week in days" class="table_body"><td ng-repeat="day in week" ng-class="{'active':day.active, 'selected':day.selected, 'other':day.other}" data-date="{{day.date}}" ng-click="pickDate(day, $event)" class="day flushMousedown"> <div class="dayNumber">{{day.date}}</div></td></tr></table></div>