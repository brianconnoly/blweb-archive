overflow: hidden
    
margin: 0 auto
margin-top 20px

width 300px

.registerCheckbox
    border 1px solid #fff
    
    width 25px
    height 25px

    margin-left: 10px
    margin-right: 15px
    float: left
    
    cursor: pointer
        
    &.checked
        .check
            display: block

    .check
        display: none
        margin: 4px auto

.registerText
    float: left
    font-size 14px
    line-height: 14px
    color: #fff
    width 230px
    text-align left