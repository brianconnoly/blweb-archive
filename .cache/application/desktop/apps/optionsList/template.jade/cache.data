<div class="window dragHandler"><div ng-bind-html="localization.translate(state.text) || state.realText" class="message"></div><div ng-if="state.description" class="description">{{localization.translate(state.description)}}</div><ul class="options"><li ng-repeat="option in state.options" ng-class="{'active':$index==state.selected}" ng-click="callAction($index)" ng-mousemove="select($index)" class="option {{option.class}}">{{localization.translate( option.text ) || option.realText}}</li></ul></div>