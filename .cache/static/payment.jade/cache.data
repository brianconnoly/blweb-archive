}
div.round {
    position: relative;
    margin: 0 15px;
    float: left;
    width: 100px;
    height: 100px;
    border-radius: 50%;
    background: black;
    opacity: 0;
    -webkit-animation: toggle 1.7s linear 0.1s infinite;
    -moz-animation: toggle 1.7s linear 0.1s infinite;
    -o-animation: toggle 1.7s linear 0.1s infinite;
    animation: toggle 1.7s linear 0.1s infinite;
}
