<div ng-if="complexMenu.showed" ng-style="{'top':complexMenu.position.top, 'left':complexMenu.position.left, 'right':complexMenu.position.right, 'height':complexMenu.position.height}" class="menuFrame"><div ng-repeat="section in complexMenu.sections" class="menuSection"><div ng-repeat="item in section.items" ng-click="clickItem(item, section, $event)" ng-class="{'disabled':item.disabled}" class="menuItem"><span translate="item.phrase"></span><span>{{item.title}}</span><div ng-if="section.type == 'checkbox'" ng-show="section.object[item.param]" class="check"></div><div ng-if="section.type == 'select' &amp;&amp; selected(item, section)" class="check"></div></div></div></div>