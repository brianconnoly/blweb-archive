<table><tr class="head"><td ng-click="goLeft()" colspan="2" class="arrow left flushMousedown"></td><td colspan="3" class="center">{{localization.translate(154 + current.month)[0]}} {{current.year}}</td><td ng-click="goRight()" colspan="2" class="arrow right flushMousedown"></td></tr><tr class="week"><td translate="148" index="1" class="day dayname"></td><td translate="149" index="1" class="day dayname"></td><td translate="150" index="1" class="day dayname"></td><td translate="151" index="1" class="day dayname"></td><td translate="152" index="1" class="day dayname"></td><td translate="153" index="1" class="day dayname"></td><td translate="147" index="1" class="day dayname"></td></tr><tr ng-repeat="week in days" class="body"><td ng-repeat="day in week" ng-class="{'active':day.active, 'selected':day.selected}" data-date="{{day.date}}" ng-click="pickDate(day, $event)" class="day flushMousedown">{{day.date}}</td></tr></table><div ng-click="hide()" class="removeBox"></div>