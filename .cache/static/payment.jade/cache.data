/* loadeingAnimations/circles.css */
@keyframes toggle {
    0%   {opacity: 0}
    30%  {opacity: 1}
    100% {opacity: 0}
}
@-moz-keyframes toggle {
    0%   {opacity: 0}
    30%  {opacity: 1}
    100% {opacity: 0}
}
@-o-keyframes toggle {
    0%   {opacity: 0}
    30%  {opacity: 1}
    100% {opacity: 0}
}
@-webkit-keyframes toggle {
    0%   {opacity: 0}
    30%  {opacity: 1}
    100% {opacity: 0}
}
div#rounds {
    position: absolute;
    top: 48%;
    left: 50%;
