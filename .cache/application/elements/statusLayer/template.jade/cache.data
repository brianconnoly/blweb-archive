<div class="sendProgress"><div class="spinner"><div class="side right"><div class="circle"></div></div><div class="side left"><div class="circle"></div></div></div><div class="comment"></div><div ng-if="sched.status == 'error'" class="error"><div class="code">{{::sched.errorDetails.code}}</div><div translate="'rpc_error_'+sched.errorDetails.code" index="text" class="description"></div></div></div><div class="schedStats"><div ng-switch="$parent.$parent.session.statsType" ng-click="showGraph(sched)" ng-if="sched.stats.length &gt; 0" class="stats"><div ng-switch-when="real" class="statsBlock white"><div class="statsLabel likes"> <div class="statsValue">{{simpleNumber(sched.lastStats.likes)}}</div><div class="statsIcon"></div></div><div class="statsLabel reposts"><div class="statsValue">{{simpleNumber(sched.lastStats.reposts)}}</div><div class="statsIcon"></div></div><div class="statsLabel comments"><div class="statsValue">{{simpleNumber(sched.lastStats.comments)}}</div><div class="statsIcon"></div></div><div class="statsLabel commentsLikes"><div class="statsValue">{{simpleNumber(sched.lastStats.commLikes)}}</div><div class="statsIcon"></div></div></div><div ng-switch-when="percent" class="percentBlock"><div ng-class="{'down':sched.delta.activity&lt;0,'zero':(sched.delta.activity*1)==0}" class="cornerIndicator"></div><div class="value">{{makeDiff(sched.delta.activity)}}</div></div></div></div>