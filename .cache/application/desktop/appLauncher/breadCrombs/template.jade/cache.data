<div class="breadCrombs dragHandler"><div ng-repeat="step in stepStack" ng-class="{'active':step==currentStep}" class="cromb"><div ng-if="step.translateTitle" translate="step.translateTitle" ng-click="jumpStep(step)" class="text"></div><div ng-if="!step.translateTitle" ng-click="jumpStep(step)" class="text">{{step.title}}</div><div ng-if="!lastStep(step)" class="arrow"></div></div></div>