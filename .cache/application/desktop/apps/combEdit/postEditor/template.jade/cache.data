<div no-effects="no-effects" dropable-item="post" type="post" id="{{id}}" ng-class="{'warn':post.readyToSchedule == false}" class="postEditor droppableItem contextMenu">   <div class="postSettings"><div class="postActions"><div ng-click="deletePost()" class="actionIcon flushMousedown deletePost"></div><div ng-click="postActions($event)" class="actionIcon flushMousedown moreActions"></div><div ng-class="post.needWork" ng-if="post.needWork" class="flushMousedown needWorkMark"></div></div><div ng-click="showNetworkList($event)" ng-style="{'background-color': networksData[post.socialNetwork].background}" class="postNetwork">{{post.socialNetwork.toUpperCase()}}</div><div class="postShortcuts"><div ng-click="newText()" class="actionIcon flushMousedown newText"></div><div ng-click="hotUpload()" class="actionIcon flushMousedown upload"></div><div ng-click="changeOrder(1)" ng-if="filterSettings.sortBy=='order'" class="actionIcon flushMousedown moveUp"></div><div ng-click="changeOrder(-1)" ng-if="filterSettings.sortBy=='order'" class="actionIcon flushMousedown moveDown"></div></div></div><div class="postContent zoom_min multiselect"><div ng-repeat="block in postContent | orderBy:'order'" ng-class="{'fullSized':isFullSized(block)}" ng-if="block.list.length &gt; 0" class="itemsContainer typeContainer {{block.type}}"><div ng-repeat="id in block.list" id="{{id}}" context="post" type="content" class="previewContainer contextMenu selectableItem editableItem droppableItem"><div type="content" id="id" class="itemPreview"></div><div ng-click="removeFromPost(id)" class="flushMousedown removeFromPost"></div><div ng-if="block.type == 'image' &amp;&amp; post.picComments[id]" ng-dblclick="openEditor(post.picComments[id], $event)" class="flushMousedown contentComment">{{commentText}}<a ng-click="removeContentDescription(id)" ng-hide="post.locked" class="flushMousedown removeFromPost"></a></div></div></div><div ng-if="emptyPost" translate="'combEditApp_empty_post_notice'" class="emptyPost"></div></div><div class="postSchedules"><div ng-repeat="schedule in schedules | orderBy:'scheduleType'" class="postSchedule"><div class="postActions"><div ng-click="removeSchedule()" class="actionIcon flushMousedown deletePost"></div><div ng-click="scheduleAction($event)" class="actionIcon flushMousedown moreActions"></div></div><div class="communityIcon"><img src="{{::community.photo}}" class="picPreload"/></div><div class="scheduleDetails"><div class="title">{{::community.name}}</div><div class="scheduleTime"><div ng-model="schedule.timestamp" ng-change="saveTime()" class="timePicker tl_postpickertime"></div><div ng-model="schedule.timestamp" dateonly="true" options="rightSide" ng-change="saveTime()" class="datePicker"> <div>{{date | timestampMask: "DD"}}</div><div>{{date | timestampMask: "MMM"}}</div><div>{{date | timestampMask: "YYYY"}}</div></div></div></div><div ng-click="sendNow($event)" title="Отправить сейчас" class="sendNowButton flushMousedown"></div><div ng-if="schedule.scheduleType == 'post'" ng-click="$parent.graphShown = true" class="statsBlock"><div ng-if="schedule.lastStats.likes &gt; 0" ng-class="{'inactive':showGhaph['likes']==false}" ng-click="triggerStat('likes')" class="statsLabel likes"><div class="statsIcon"></div><div class="statsValue">{{schedule.lastStats.likes | formatNumber}}</div></div><div ng-if="schedule.lastStats.reposts &gt; 0" ng-class="{'inactive':showGhaph['reposts']==false}" ng-click="triggerStat('reposts')" class="statsLabel reposts"><div class="statsIcon"></div><div class="statsValue">{{schedule.lastStats.reposts | formatNumber}}</div></div><div ng-if="schedule.lastStats.comments &gt; 0" ng-class="{'inactive':showGhaph['comments']==false}" ng-click="triggerStat('comments')" class="statsLabel comments"><div class="statsIcon"></div><div class="statsValue">{{schedule.lastStats.comments | formatNumber}}</div></div><div ng-if="schedule.lastStats.commLikes &gt; 0" ng-class="{'inactive':showGhaph['commLikes']==false}" ng-click="triggerStat('commLikes')" class="statsLabel commentsLikes"><div class="statsIcon"></div><div class="statsValue">{{schedule.lastStats.commLikes | formatNumber}}</div></div></div><div ng-if="graphShown" class="statsGraph"><canvas width="308" height="160"></canvas><div class="values"></div><div class="helper"></div><div ng-click="hideGraph()" class="hideButton">Скрыть</div><div ng-click="switchScale(!scale)" class="graphSwitcher"><div ng-if="scale == true" translate="'postEdit_graph_display_real'" class="value"></div><div ng-if="scale != true" translate="'postEdit_graph_scale_to_height'" class="value"></div></div></div></div></div></div>