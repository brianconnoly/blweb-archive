tC['/novaAudioPreview/template.jade'] = "<div class=\"icon\"><img src=\"/resources/images/nova/entities/note.svg\"/></div> <div class=\"caption\"> <div class=\"wrapper name\"> <div class=\"track\"> {{item.title}}</div> <div class=\"artist\">{{item.artist}}</div> </div> </div>";

registerStyles('novaAudioPreview', ".novaAudioPreview {\n  position: absolute;\n  top: 0;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  overflow: hidden;\n}\n.novaAudioPreview .caption {\n  position: absolute;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  height: 50%;\n  width: 100%;\n  display: table;\n  opacity: 1;\n  -ms-filter: none;\n  filter: none;\n  -webkit-transition: opacity 0.2s ease;\n  -moz-transition: opacity 0.2s ease;\n  -o-transition: opacity 0.2s ease;\n  -ms-transition: opacity 0.2s ease;\n  transition: opacity 0.2s ease;\n}\n.noItemNames .novaAudioPreview .caption {\n  opacity: 0;\n  -ms-filter: \"progid:DXImageTransform.Microsoft.Alpha(Opacity=0)\";\n  filter: alpha(opacity=0);\n}\n.novaAudioPreview .caption .wrapper {\n  width: 100%;\n  display: table-cell;\n  vertical-align: middle;\n  text-align: center;\n}\n.novaAudioPreview .caption .wrapper .artist {\n  -o-text-overflow: ellipsis;\n  text-overflow: ellipsis;\n  overflow: hidden;\n  white-space: nowrap;\n}\n.novaAudioPreview .caption .wrapper .track {\n  font-weight: bold;\n  margin-bottom: 1%;\n}\n.novaAudioPreview .icon {\n  position: absolute;\n  left: 0;\n  right: 0;\n  bottom: 50%;\n  max-height: 40%;\n  text-align: center;\n}\n.noItemNames .novaAudioPreview .icon {\n  bottom: 10%;\n  min-height: 80%;\n}\n.novaAudioPreview .icon img {\n  height: 100%;\n}\n");
