<div class="touchHelper"><div ng-click="toRight()" ng-class="{'disabled':multiselect.focusedHash.length &lt; 1}" class="button right ios-nodelay"><span class="name">{{localization.translate('touch_rightpanel')}}</span></div><div ng-click="back()" class="button back ios-nodelay"><span class="name">{{localization.translate('touch_back')}}</span></div><div ng-class="{'active':state.multiselect}" ng-click="triggerMultiselect()" class="button multiselect ios-nodelay"><span class="name">{{localization.translate('touch_multiselect')}}</span></div></div>