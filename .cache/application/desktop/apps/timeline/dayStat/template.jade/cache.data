<div translate="dateInfo.weekDay" index="0" class="weekDay"></div><div class="monthYear"><span translate="dateInfo.month" index="2"></span><span> {{::dateInfo.year}}</span></div><div class="monthDay">{{::dateInfo.day}}</div><div ng-if="hasStats" ng-click="launchGraph($event)" class="stats"><div class="value"><span ng-class="{'down':down}" class="marker"></span><span class="percent">{{value}}%</span></div><div translate="'statPeriod_'+period" class="period"></div></div><div ng-click="right($event)" ng-if="canScroll" class="flushMousedown arrow arrowRight"><div class="corner"></div></div><div ng-click="left($event)" ng-if="canScroll" class="flushMousedown arrow arrowLeft"><div class="corner"></div></div>