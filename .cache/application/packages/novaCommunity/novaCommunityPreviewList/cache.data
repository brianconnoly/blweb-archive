tC['/novaCommunityPreviewList/template.jade'] = "<div title=\"{{::item.name}}\" class=\"cover\"><img src=\"{{item.photo}}\" class=\"picPreload\"/> <div ng-style=\"{background: networksData[item.socialNetwork].background}\" class=\"snType\">{{::item.socialNetwork}}</div> </div> <div class=\"parameters\"> <div class=\"parameter\"> <div class=\"label\">{{::item.name}}</div> </div> </div>";

registerStyles('novaCommunityPreviewList', ".novaCommunityPreviewList {\n  width: 100%;\n  overflow: hidden;\n  padding: 10px 0;\n}\n.novaCommunityPreviewList > .cover {\n  width: 50px;\n  height: 50px;\n  margin: 0 15px;\n  float: left;\n  background: #d4d4d4;\n  -webkit-border-radius: 3px;\n  border-radius: 3px;\n  -webkit-transform: translate3d(0, 0, 0);\n  -moz-transform: translate3d(0, 0, 0);\n  -o-transform: translate3d(0, 0, 0);\n  -ms-transform: translate3d(0, 0, 0);\n  transform: translate3d(0, 0, 0);\n  overflow: hidden;\n  -webkit-box-shadow: 0 0 2px rgba(0,0,0,0.4);\n  box-shadow: 0 0 2px rgba(0,0,0,0.4);\n}\n.novaCommunityPreviewList > .cover .userPic {\n  -webkit-border-radius: 0;\n  border-radius: 0;\n}\n.novaCommunityPreviewList .parameters {\n  position: absolute;\n  top: 10px;\n  right: 15px;\n  left: 80px;\n  bottom: 10px;\n}\n.novaCommunityPreviewList .parameters .parameter {\n  overflow: hidden;\n  font-size: 14px;\n  line-height: 15px;\n  color: #000;\n}\n.novaCommunityPreviewList .parameters .parameter .value {\n  float: right;\n}\n");
