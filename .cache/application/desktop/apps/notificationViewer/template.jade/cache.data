<div class="dragHandler topBar black noMaximize noSettings"><process-bar></process-bar><div class="appControls"></div><div translate="'notificationViewerApp_title'" class="title"></div></div><div class="body dragHandler"><div class="list"><div ng-repeat="notification in notificationsList" class="notificationItem"><div class="notificationPreview"></div></div></div><div class="paginator"><div ng-click="pageRight()" ng-show="currentPage &lt; totalPages-1" class="activator right"><img src="/resources/images/elements/arrow-right.svg" width="11px" height="20px"/></div><div ng-click="pageLeft()" ng-show="currentPage &gt; 0" class="activator left"><img src="/resources/images/elements/arrow-left.svg" width="11px" height="20px"/></div><div ng-repeat="page in makePaginatorFromPages(totalPages, currentPage, 5)" ng-class="{'active':currentPage==page}" ng-click="goPage(page)" class="activator goPage">{{page+1}}</div></div></div>