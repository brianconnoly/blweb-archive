registerStyles('novaScreen', ".novaScreen {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  bottom: 0;\n  -webkit-transition: left 0.2s ease;\n  -moz-transition: left 0.2s ease;\n  -o-transition: left 0.2s ease;\n  -ms-transition: left 0.2s ease;\n  transition: left 0.2s ease;\n}\n.novaScreen.hideLeft {\n  left: -100%;\n}\n.novaScreen.hideRight {\n  left: 100%;\n}\n.novaScreen.picked .screenBar .backScreen {\n  margin-bottom: 5px;\n}\n.novaScreen.picked .screenBar .novaScreenTextHelper {\n  margin-top: 5px;\n}\n.novaScreen .screenBar {\n  padding: 20px 0;\n  padding-bottom: 10px;\n  width: inherit;\n  font-size: 14px;\n  color: rgba(0,0,0,0.5);\n  position: fixed;\n  z-index: 2;\n  top: 0;\n  background: rgba(255,255,255,0.95);\n}\n.novaScreen .screenBar .backScreen {\n  margin-left: 20px;\n  margin-bottom: 10px;\n  cursor: pointer;\n}\n.novaScreen .screenBar .novaScreenTextHelper {\n  color: #000;\n  font-size: 14px;\n  margin-top: 10px;\n  margin-left: 20px;\n}\n");
