<div id="combEdit" item="state.currentComb" ng-controller="CombEditCtrl" class="contextMenu fullScreenOverlay overlayLayout"><div class="filterBar"><div ng-click="onEscape()" class="closer"></div><div ng-show="onlyOnePost()" class="name"><input on-input="saveName" settings="{blurOnEnter:true, timeout:2000}" ng-model="state.currentComb.name" placeholder="{{translate('noname')}}"/></div><div ng-show="!onlyOnePost()" class="filtersBlock"><div class="content"><switcher ng-model="contentView" ng-change="updateContentView()" class="contentViewSwitcher"><div option="full" class="button full"></div><div option="mini" class="button mini"></div></switcher><div class="name"><input on-input="saveName" settings="{blurOnEnter:true, timeout:2000}" ng-model="state.currentComb.name" placeholder="{{translate('noname')}}"/></div></div><div class="posts"><div ng-hide="postId" class="postsFilters"><combobox ng-model="postSched" list="filterList" ng-change="setFilter()" class="filter"></combobox><combobox ng-model="postSort" list="sortList" class="sort"></combobox><switcher ng-model="postView" ng-show="!postId" ng-change="updatePostView()" class="postsViewSwitcher"><div option="full" class="button full"></div><div option="simple" class="button simple"></div></switcher></div><div ng-click="closePost()" ng-show="postId" class="button backward">{{localization.translate('comb_view_posts_back')}}</div></div></div></div><div class="container multiselect"><div no-effects="no-effects" dropable-item="state.currentComb" ng-class="{'alone': onlyOnePost(), 'teamShown':state.currentComb.teamId}" ng-click="hideMenu()" class="combeditor animated droppableItem"><div scroll-top="white" antiscroll="antiscroll" ng-class="{'mini':contentView=='mini'}" ng-switch="contentList" class="column ios-scroll content"><div ng-switch-when="scroll" class="contentMode"><div ng-repeat="(category, items) in state.currentComb.contentIds" ng-show="items.length &gt; 0" class="category"><ul ng-style="{'width':contentWidth}" class="list"><li type="{{category}}" ng-repeat="id in items | reverse" ng-dblclick="contentClick(item, $event)" class="contextMenu item ios-nodelay selectableItem editableItem droppableItem"><div ng-dblclick="contentClick(id, $event)" type="content" id="id" class="itemPreview"></div></li></ul></div></div><div ng-switch-when="pagination" class="contentMode"><div class="contentPaginatorView"><ul ng-style="{'width':contentWidth}" class="list"><li type="content" ng-repeat="id in getPaginatorPage()" ng-dblclick="contentClick(item, $event)" class="contextMenu item ios-nodelay selectableItem editableItem droppableItem"><div ng-dblclick="contentClick(id, $event)" type="content" id="id" class="itemPreview"></div></li></ul></div><div class="superPaginator doubleArrows"></div></div></div><div no-effects="no-effects" dropable-item="{'type':'postPanel','comb':state.currentComb}" scroll-top="white" ng-switch="postView" class="column ios-scroll posts droppableItem"><div ng-switch-when="simple" ng-class="{'postEditMode':postId!=null}" class="postMode"><ul antiscroll="antiscroll" class="list simplePostList size75"><li ng-repeat="id in filterPosts(state.currentComb.postIds, currentPage, true)" type="post" ng-dblclick="postClick(id)" class="item contextMenu selectableItem droppableItem"><div type="post" id="id" class="itemPreview"></div></li></ul><div antiscroll="antiscroll" class="postEdit"><post-entity ng-if="postId != null"></post-entity></div><div ng-show="makePaginator(filterPosts(state.currentComb.postIds, null, true), currentPage, 7, pageSize).length &gt; 1" class="paginator doubleArrows"><div ng-click="prevPage()" ng-show="currentPage &gt; 0" class="arrow left"></div><div ng-click="nextPage()" ng-show="currentPage &lt; (filterPosts(state.currentComb.postIds, null, true).length/pageSize)-1" class="arrow right"></div><div ng-click="goPage(0)" ng-show="currentPage &gt; 0" class="doubleArrow left"></div><div ng-click="goPage((filterPosts(state.currentComb.postIds).length/pageSize)-1)" ng-show="currentPage &lt; (filterPosts(state.currentComb.postIds).length/pageSize)-1" class="doubleArrow right"></div><div class="centerPages"><div ng-repeat="page in makePaginator(filterPosts(state.currentComb.postIds, null, true), currentPage, 7, pageSize)" ng-class="{'active':page == currentPage}" ng-click="goPage(page)" class="page">{{pageNum(page, makePaginator(filterPosts(state.currentComb.postIds, null, true), currentPage, 7, pageSize).length)}}</div></div></div></div><div ng-switch-default="ng-switch-default" class="postMode"><ul antiscroll="antiscroll" class="list fullPosts"><post-entity ng-repeat="postId in filterPosts(state.currentComb.postIds, currentPage)"></post-entity></ul><div ng-show="makePaginator(filterPosts(state.currentComb.postIds)).length &gt; 1" class="paginator doubleArrows"><div ng-click="prevPage()" ng-show="currentPage &gt; 0" class="arrow left"></div><div ng-click="nextPage()" ng-show="currentPage &lt; (filterPosts(state.currentComb.postIds).length/5)-1" class="arrow right"></div><div ng-click="goPage(0)" ng-show="currentPage &gt; 0" class="doubleArrow left"></div><div ng-click="goPage((filterPosts(state.currentComb.postIds).length/5)-1)" ng-show="currentPage &lt; (filterPosts(state.currentComb.postIds).length/5)-1" class="doubleArrow right"></div><div class="centerPages"><div ng-repeat="page in makePaginator(filterPosts(state.currentComb.postIds), currentPage)" ng-class="{'active':page == currentPage}" ng-click="goPage(page)" class="page">{{pageNum(page, makePaginator(filterPosts(state.currentComb.postIds)).length)}}</div></div></div></div></div><div ng-if="state.currentComb.teamId" team-id="state.currentComb.teamId" class="boundedTeamMembers multiselect"></div></div></div></div>