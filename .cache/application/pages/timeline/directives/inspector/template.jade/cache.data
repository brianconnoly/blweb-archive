<div ng-click="closeInspector()" class="removeBox"></div><div ng-show="status.multiEdit" class="multiEditPopup"><div class="line selected"><div class="capture wideCapture">{{localization.translate('inspector_selected_count')}}:</div><div class="value">{{status.selectedPlaceholders.length}}</div></div><div class="line repeat"><div class="capture wideCapture">{{localization.translate('inspector_set_rule_rype')}}:</div><div class="value"><div ng-click="setMultiPlan('single')" ng-class="{'cur':status.multiType=='single'}" title="{{localization.translate('inspector_make_single')}}" class="single multi_single"><div></div></div><div ng-click="setMultiPlan('daily')" ng-class="{'cur':status.multiType=='daily'}" title="{{localization.translate('inspector_make_daily')}}" class="daily"><div></div></div></div></div><div ng-show="status.multiType"><div ng-show="status.multiType=='daily'" class="dailyselector"><div class="capture">{{localization.translate('inspector_days_repeat')}}</div><div class="value"><div ng-repeat="day in daysArray" ng-click="selectDay($event, day)" ng-class="getDayClass(day)" class="day"> <div class="square">{{daysValues[day]}}</div><div class="plane"></div></div></div></div><div ng-show="status.multiType!=''" class="line"><div class="capture wideCapture">{{localization.translate('inspector_ad')}}</div><div class="value v_advert"><checkbox ng-model="status.multiAd" ng-change="saveMulti()" class="long"></checkbox></div></div><div ng-show="status.multiType!='single'" class="line"><div class="capture wideCapture">{{localization.translate('inspector_ending')}}</div><div class="value v_advert"><checkbox ng-model="status.multiEnd" class="long"></checkbox></div></div><div ng-show="status.multiType!='single' &amp;&amp; status.multiEnd" class="line"><div ng-dblclick="scrollTo(status.multiEndDay)" class="capture captureShift">{{localization.translate('inspector_timestamp_end')}}</div><div class="value"><div ng-model="status.multiEndDay" dateonly="true" options="inspector" ng-change="saveMulti()" class="date datePicker">{{status.multiEndDay | timestampMask: "DD MMM YYYY"}}</div></div></div></div><div ng-click="setFocusRule()" ng-show="selected.length==1" class="line focusRule"><div style="width:200px" class="capture wideCapture selectDay">Выбрать правило</div></div></div><div ng-show="!status.multiEdit" class="singleEditPopup"><div class="line"><div ng-dblclick="scrollTo(startDay)" class="capture captureShift">{{localization.translate('inspector_timestamp_start')}}</div><div class="value"><div ng-model="status.currentRule.timestampStart" ng-change="saveRule()" min="5" class="timePicker timePickerInspecor"></div><div ng-model="status.currentRule.timestampStart" dateonly="true" options="inspector" ng-change="saveRule()" class="date datePicker">{{status.currentRule.timestampStart | timestampMask: "DD MMM YYYY"}}</div></div></div><div class="line repeat"><div class="capture wideCapture">{{localization.translate('inspector_repeat_type')}}</div><div class="value"><div ng-click="setPlan('single')" ng-class="{'cur':status.currentRule.ruleType=='single'}" title="{{localization.translate('inspector_single')}}" class="single"><div></div></div><div ng-click="setPlan('daily')" ng-class="{'cur':status.currentRule.ruleType=='daily'}" title="{{localization.translate('inspector_daily')}}" class="daily"><div></div></div><div ng-click="setPlan('chain')" ng-class="{'cur':status.currentRule.ruleType=='chain'}" title="{{localization.translate('inspector_chain')}}" class="chain"><div></div></div></div></div><div ng-show="status.currentRule.ruleType=='chain'" class="line interval"><div class="capture wideCapture captureShift">{{localization.translate('inspector_interval')}}</div><div class="value"><div ng-model="status.currentRule.interval" options="inspectorInterval" ng-change="saveRule()" min="5" class="timePicker timePickerInspecor"></div></div><div class="clear"></div></div><div ng-show="status.currentRule.ruleType=='daily'" class="dailyselector"><div class="capture">{{localization.translate('inspector_days_repeat')}}</div><div class="value"><div ng-repeat="day in daysArray" ng-click="selectDay($event, day)" ng-class="getDayClass(day)" class="day"> <div class="square">{{daysValues[day]}}</div><div class="plane"></div></div></div></div><div ng-show="status.currentRule.ruleType!='chain'" class="line"><div class="capture wideCapture">{{localization.translate('inspector_ad')}}</div><div class="value v_advert"><checkbox ng-model="status.currentRule.ad" ng-change="saveRule()" class="long"></checkbox></div></div><div ng-show="status.currentRule.ruleType!='single'" class="line"><div class="capture wideCapture">{{localization.translate('inspector_ending')}}</div><div class="value v_advert"><checkbox ng-model="status.currentRule.end" ng-change="saveRule()" class="long"></checkbox></div></div><div ng-show="status.currentRule.ruleType!='single' &amp;&amp; status.currentRule.end" class="line"><div ng-dblclick="scrollTo(endDay)" class="capture captureShift">{{localization.translate('inspector_timestamp_end')}}</div><div class="value"><div ng-model="status.currentRule.timestampEnd" ng-show="status.currentRule.ruleType!='daily'" ng-change="saveRule()" class="timePicker timePickerInspecor"></div><div ng-model="status.currentRule.timestampEnd" dateonly="true" options="inspector" edgetime="status.currentRule.timestampStart" ng-change="saveRule()" class="date datePicker">{{status.currentRule.timestampEnd | timestampMask: "DD MMM YYYY"}}</div></div></div><div ng-show="status.currentRule.ruleType=='chain'" ng-click="focusDay()" class="line focusDay"><div class="capture wideCapture selectDay">{{localization.translate('select_day')}}</div></div><div ng-click="removeAll()" class="remove">{{localization.translate('inspector_removeAll')}}</div></div>