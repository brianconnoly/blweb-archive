<div class="previews"><div type="lot" id="notification.itemList[0].id" class="itemPreview"></div></div><div class="date">{{notification.created | timestampMask: "DD ofMMMM YYYY hh:mm"}}</div><div class="text"> <div class="header"><span ng-if="notification.result == true">{{translate('marketRequest_accept')}}</span><span ng-if="notification.result != true">{{translate('marketRequest_failed')}}</span></div><div class="description"><span ng-if="notification.result == true">{{translate('marketRequest_accept_description')}}</span><span ng-if="notification.result != true">{{translate('marketRequest_failed_description' + notification.status)}}</span></div></div><div class="notificationButtons"></div>