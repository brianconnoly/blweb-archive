<li class="interval liveInterval"><div ng-repeat="part in interval.parts" style="left: {{part.left}}px; width: {{part.width}}px;" ng-switch="part.type" class="part"><div ng-switch-when="dayBreak" ng-class="{'glued':group.glued==2}" class="dayBreak"><div translate="part.timeInfo.weekDay" index="0" class="weekDay"></div><div class="date">{{::part.timeInfo.day}}</div><div translate="part.timeInfo.month" index="0" class="month"></div><div class="year">{{::part.timeInfo.year}}</div></div><div ng-switch-when="hourMark" ng-class="{'glued':group.glued}" class="hourMark">{{::part.humanTime}}</div><div ng-switch-when="freePlace" ng-hide="group.glued" class="freePlace"><div class="column"><div class="smallMark"></div></div><div class="column"><div class="smallMark"></div></div><div class="column"><div class="smallMark"></div></div><div class="column"><div class="longMark"></div></div><div class="column"><div class="smallMark"></div></div><div class="column"><div class="smallMark"></div></div><div class="column"><div class="smallMark"></div></div><div class="column"><div class="longMark"></div></div><div class="column"><div class="smallMark"></div></div><div class="column"><div class="smallMark"></div></div><div class="column"><div class="smallMark"></div></div></div><div ng-switch-when="regular" class="freePlace"><div ng-repeat="mark in part.marks" style="width:{{::mark.width}}px; left:{{::mark.left}}px;" class="column absolute"><div ng-hide="mark.hide" class="smallMark"></div></div></div></div></li>