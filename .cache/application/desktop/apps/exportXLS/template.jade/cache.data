<div class="dragHandler topBar noMaximize noSettings black"><process-bar></process-bar><div class="appControls"></div><div translate="'exportXLSApp_title'" class="title"></div></div><div class="body"><div class="exportSettings"><div class="community"><div item="community" class="itemPreview"></div></div><div class="exportParams"><div translate="'exportXLSApp_title'" class="caption"></div><input ng-model="params.forcedName" class="value"/><div translate="'exportXLSApp_from'" class="caption"></div><div class="pickerContainer value"><div ng-model="params.from" ng-change="changedDate()" class="timePicker"></div><div ng-model="params.from" ng-change="changedDate()" dateonly="true" class="datePicker">{{params.from | timestampMask: "DD MMM YYYY"}}</div></div><div translate="'exportXLSApp_to'" class="caption"></div><div class="pickerContainer value"><div ng-model="params.to" ng-change="changedDate()" class="timePicker"></div><div ng-model="params.to" ng-change="changedDate()" dateonly="true" class="datePicker">{{params.to | timestampMask: "DD MMM YYYY"}}</div></div></div></div><div class="presets"><span ng-repeat="preset in presets" ng-class="{'active':activePreset == preset}" class="preset"><span ng-click="setPreset(preset)" translate="'exportXLSApp_preset_'+preset" class="link"></span></span></div><ul class="options"><li translate="'exportXLSApp_doExport'" ng-click="doExport()" class="hoverActive option"></li></ul></div>