<div ng-hide="showContentList" antiscroll="antiscroll" class="screen widgets multiselect"><ul class="widgetList"><li ng-repeat="widget in widgets" ng-class="{'empty':!pages.length, 'singlePage':pages.length==1}" class="filterWidget"></li></ul></div><div ng-show="showContentList" antiscroll="antiscroll" class="screen items multiselect"><!--div.back(ng-click="back()", ng-show="widgets != null") Back--><h2 class="headlining title"><div ng-click="back()" ng-show="widgets != null" class="back">Back</div><div class="caption">{{localization.translate(viewSection.title)}}</div><div ng-if="viewSection.communityId" community-id="viewSection.communityId" class="communityTitle"></div></h2><div ng-style="{'width':widgetWidth}" ng-class="{'small_view':params.smallIcons}" class="sideMargin content"><div ng-click="goRight()" ng-hide="noRight" class="arrow right"></div><div ng-click="goLeft()" ng-hide="noLeft" class="arrow left"></div><div style="margin: 0 40px"><ul ng-style="{'height':widgetHeight, 'width':contentWidth}" class="list"><li ng-repeat="item in items" class="item contextMenu selectableItem editableItem droppableItem"><div item="item" overlay="catalog" class="itemPreview"></div></li></ul></div></div><div ng-show="pages&gt;1" class="pagination arrows"><div ng-click="goPage(0)" ng-hide="noLeft" class="doubleArrow left"></div><div ng-click="goPage(pages-1)" ng-hide="noRight" class="doubleArrow right"></div><div class="centerPages"><div ng-repeat="page in paginator(pages, currentPage)" ng-class="{'active':page==currentPage}" ng-click="goPage(page)" class="marker"><div ng-if="pages&lt;=5">â€¢</div><div ng-if="pages&gt;5">{{page+1}}</div></div></div></div></div>