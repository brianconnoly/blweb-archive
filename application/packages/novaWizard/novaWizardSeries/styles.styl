overflow: hidden;
.novaWizardSeriesPicker
    padding 5px 0px //25px
    overflow: hidden;

    &.lastFixed
        background-color: rgba(255,255,255,0.9);
        box-shadow: 126px 2px 5px -2px rgba(0,0,0,0.2);

    &.fixed
        height 90px
        width 100%
        overflow: hidden;
        position: fixed;
        z-index: 1;

        .pickerItem *
            cursor default
        div.parameter
            .title
                position relative

    div.parameter
        margin 0

        .title
            position fixed
