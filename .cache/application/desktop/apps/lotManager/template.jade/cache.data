<div class="dragHandler topBar noMaximize noSettings black"><process-bar></process-bar><div class="appControls"></div><div ng-click="stepBack()" ng-if="canGoBack()" class="backButton"></div><div ng-if="currentStep.translateTitle" translate="currentStep.translateTitle" class="title"></div><div ng-if="!currentStep.translateTitle" class="title">{{currentStep.title}}</div></div><div class="body panelWide"><div scroll-value="lotScrollValue" antiscroll="antiscroll" class="leftPanel itemsContainer lazyLots rememberScroll zoom_mid"><div ng-repeat="item in requestLots | filter:lotFilter | orderBy:lotOrder:true " ng-class="{'active': item == currentLot}" ng-click="selectLot(item)" class="panelItem"><div ng-class="{'active':item.userUpdated[0].unread, 'counter':item.userUpdated[0].updates &gt; 0}" class="notify"> <span ng-if="item.userUpdated[0].updates &gt; 0">{{item.userUpdated[0].updates}}</span></div><div class="previewContainer"><div item="item" class="itemPreview"></div></div></div></div><div ng-class="{'showSettings':showSettings &amp;&amp; currentLot.userId == user.id, 'appSettingsLayout_showed':currentLot &amp;&amp; currentLot.userId == user.id}" class="rightContent itemsContainer multiselect appSettingsLayout"><div class="appSettingsLayout_topBar currentLot"><div class="params"><div ng-click="showSettings = !showSettings" class="settings gearIcon flushMousedown appSettingsLayout_activator"></div><checkbox ng-class="{'active':lotActive()}" ng-model="currentLot.published" ng-change="triggerPublish()" class="long"></checkbox><span ng-class="{'active':lotActive()}" ng-click="triggerPublish(true)" class="published"><span translate="'lotManagerApp_lotPublished'" ng-show="currentLot.published &amp;&amp; currentLot.moderationStatus == 'accepted'"></span><span translate="'lotManagerApp_lotPublish'" ng-show="!currentLot.published"></span><span translate="'lotManagerApp_lotWaiting'" ng-show="currentLot.published &amp;&amp; currentLot.moderationStatus == 'notModerated'"></span><span translate="'lotManagerApp_lotRejected'" ng-show="!currentLot.published &amp;&amp; currentLot.moderationStatus == 'rejected'"></span></span></div><div ng-if="lotCommunity" class="smallPreview"><img src="{{lotCommunity.photo}}" class="picPreload"/></div><div class="title">{{lotCommunity.name}}</div></div><div class="appSettingsLayout_settings lotSettings"><div ng-if="userMorpheus()" class="buzzlot"><span>BL</span><checkbox ng-model="currentLot.buzzLot" ng-change="saveLot()" class="long"></checkbox></div><div class="previewColumn itemsContainer zoom_max"><div dropable-item="currentLot" class="previewContainer droppableItem"><div type="post" id="currentLot.postId" class="itemPreview"></div></div><div translate="'lotManagerApp_previewLot'" ng-click="previewLot()" class="previewButton"></div><div translate="'lotManagerApp_removeLot'" ng-click="deleteLot()" class="removeButton"></div></div><div ng-class="{'disabled':currentLot.published}" class="paramsColumn"><div class="list"><div class="line"><div translate="'lotPreviewApp_lot_communitySize'" class="title"></div><div class="value">{{::lotCommunity.membersCount}} {{localization.declensionPhrase(lotCommunity.membersCount,'subscribers')}}</div></div><div class="line"><div translate="'lotPreviewApp_lot_title'" class="title"></div><div class="value"><input ng-model="currentLot.name" ng-change="saveLot()" ng-model-options="{ updateOn: 'default blur', debounce: {'default': 500, 'blur': 0} }" class="settingsInput longInput"/></div></div><div class="line"><div translate="'lotManagerApp_minSubscribers'" class="title"></div><div class="value"> <input ng-model="currentLot.minSubscribers" ng-change="saveLot()" ng-model-options="{ updateOn: 'default blur', debounce: {'default': 500, 'blur': 0} }" class="settingsInput longInput"/></div></div><div class="line"><div translate="'lotManagerApp_lotPrice'" class="title"></div><div class="value"><input ng-model="currentLot.price" ng-change="saveLot()" ng-model-options="{ updateOn: 'default blur', debounce: {'default': 500, 'blur': 0} }" class="settingsInput longInput"/></div></div><div class="line"><div translate="'lotManagerApp_lotDates'" class="title"></div><div class="value"> <div ng-model="currentLot.dateFrom" dateonly="true" ng-change="saveLot()" class="datePicker">{{currentLot.dateFrom | timestampMask: "DD.M.YY"}}</div><div class="separator">–</div><div ng-model="currentLot.dateTo" dateonly="true" ng-change="saveLot()" class="datePicker">{{currentLot.dateTo | timestampMask: "DD.M.YY"}}</div></div></div><div class="line"><div translate="'lotPreviewApp_lot_timeLimit'" class="title"></div><div class="value"> <div ng-model="currentLot.timeFrom" options="cleartime" ng-change="saveLot()" class="settingsInput shortInput timePicker"></div><div class="separator">–</div><div ng-model="currentLot.timeTo" options="cleartime" ng-change="saveLot()" class="settingsInput shortInput timePicker"></div></div></div><div class="line"><div translate="'lotManagerApp_lotGuarantee'" class="title"></div><div class="value"> <div ng-model="currentLot.checkAfter" options="cleartime" ng-change="saveLot()" class="timePicker separated"></div></div></div></div></div></div><div class="requestsList appSettingsLayout_body blTable"><div class="head"><div class="cell pic"></div><div translate="'lotManagerApp_column_community'" class="cell community"></div><div translate="'lotManagerApp_column_subscribers'" class="cell subscribers"></div><div translate="'lotManagerApp_column_price'" class="cell price"></div><div translate="'lotManagerApp_column_time'" class="cell time"></div><div translate="'lotManagerApp_column_status'" class="cell status"></div></div><div scroll-value="requestScrollValue" antiscroll="antiscroll" class="list lazyRequests rememberScroll"><div ng-repeat="item in requests | orderBy:requestOrder:true" class="tableLine requestItem"><div class="cell pic"><div ng-click="openCommPage(communityItem)" class="picBox"><img src="{{communityItem.photo}}" class="picPreload"/></div></div><div ng-dblclick="openCommPage(communityItem)" class="cell community">{{::communityItem.name}}</div><div class="cell subscribers">{{::communityItem.membersCount}}</div><div class="cell price">{{item.cost}}</div><div class="cell time">{{getFormatedDate(item.timestamp)}}</div><div class="cell status itemActionBlock"><div ng-if="!hasActions()" translate="'lotManagerApp_requestStatus_' + item.requestStatus" ng-class="item.requestStatus" class="statusBlock"></div><div ng-if="hasActions()" ng-switch="action" class="actions"><div ng-switch-when="reject" translate="'lotManagerApp_requestAction_reject'" ng-click="reject()" class="action"></div><div ng-switch-when="accept" translate="'lotManagerApp_requestAction_accept'" ng-click="accept()" class="action"></div><div ng-switch-when="cancel" translate="'lotManagerApp_requestAction_cancel'" ng-click="cancel()" class="action"></div><div ng-click="showMenu($event)" class="menu"></div></div></div></div></div></div></div></div><bread-crombs></bread-crombs>