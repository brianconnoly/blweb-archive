<div class="dragHandler topBar noSettings"><process-bar class="red"></process-bar><div class="appControls"></div><div translate="'mediaPreviewApp_title'" ng-if="!state.item.name" class="title"></div><div ng-if="state.item.name" class="title">{{state.item.name}}.{{state.item.fileType}}</div></div><div class="lightboxPreview body dragHandler"><div ng-dblclick="download($event)" class="window closer loading"><div ng-if="state.item.type=='file'" class="download flushMousedown"><div ng-click="selectUrl($event)" class="url allowContext">{{state.item.source}}</div><div ng-click="download($event)" class="button downloadButton"></div></div><div ng-show="showType == 'image'" class="picture content"><img ng-src="{{imageUrl}}" class="downloadButton"/></div><div ng-show="showType == 'video'" class="video content"></div><div ng-click="goLeft($event)" ng-if="state.item.type=='file' &amp;&amp; canGoLeft()" class="arrow left"></div><div ng-click="goRight($event)" ng-if="state.item.type=='file' &amp;&amp; canGoRight()" class="arrow right"></div></div></div>