<div class="dragHandler topBar black noSettings"><process-bar></process-bar><div class="appControls"></div><div class="title">Api testing tool</div></div><div class="call body"><div class="call_form"><div ng-show="current.controller.methods.length"><p class="form_title">Method</p><combobox list="current.controller.methods" ng-model="current.method" ng-change="setFields()" class="form_method"></combobox></div><div ng-show="fields.length" class="form_fields"><div class="form_field"><div class="field_delete"></div><span class="field_key">userId</span><span class="field_separator">:</span><input ng-model="request.userId" type="text" class="field_val"/></div><div class="form_field"><div class="field_delete"></div><span class="field_key">apiKey</span><span class="field_separator">:</span><input ng-model="request.apiKey" type="text" class="field_val"/></div><p class="form_title">Data</p><div ng-repeat="field in fields" class="form_field"><div class="field_delete"><span ng-show="field.editable" ng-click="deleteField(field)" title="Delete" class="field_delete">[X]</span></div><span ng-show="!field.editable" ng-bind="field.key" class="field_key"></span><input ng-show="field.editable" ng-model="field.key" class="field_key editable"/><span class="field_separator">:</span><input ng-model="field.value" type="{{field.type}}" class="field_val"/></div></div><button ng-disabled="!current.method" ng-click="call()" class="form_submit">Call method</button></div><div class="call_result"><button ng-click="selectAll()" class="result_select">Select all</button><pre id="result" ng-bind="result"></pre></div></div>