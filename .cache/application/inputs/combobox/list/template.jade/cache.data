<div ng-show="state.opened" ng-click="close()" ng-class="{'hideSelected': state.settings.hideSelected}" class="{{state.listClass}}"><div ng-style="state.position" antiscroll="antiscroll" class="content"><ul class="list"><li ng-repeat="item in state.list" ng-click="select(item)" ng-class="{'selected': item == state.selected}" ng-style="item.style" class="{{item.class}}"> <span ng-if="item.translateTitle">{{translate(item.translateTitle)}}</span><span ng-if="item.title">{{item.title}}</span></li></ul></div></div>