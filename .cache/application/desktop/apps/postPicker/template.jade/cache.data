<div class="dragHandler topBar noMaximize noSettings"><process-bar></process-bar><div class="appControls"></div><div translate="'postPickerApp_title'" class="title"></div></div><div class="body"><div class="scheduleInformation"><div class="communityPic"><img src="{{community.photo}}" class="picPreload"/></div><div class="info"><div class="communityName">{{community.name}}</div><div class="timeInfo"><div ng-model="state.timestamp" class="timePicker tl_postpickertime"></div><div ng-model="state.timestamp" dateonly="true" options="rightSide" class="datePicker"> <div>{{date | timestampMask: "DD"}}</div><div>{{date | timestampMask: "MMM"}}</div><div>{{date | timestampMask: "YYYY"}}</div></div></div></div></div><div antiscroll="antiscroll" class="postShelf"><div translate="'postPickerApp_createNew'" class="shelfTitle"></div><div class="upperBlocks shelf ios-scroll"><div title="{{localization.translate('postpicker_newpost')}}" class="itemSquare"><div ng-click="pick(newpost)" translate="'postPickerApp_newPost'" class="pickerItem addNew"></div></div><div ng-show="session.groupId" title="{{localization.translate('postpicker_newcomb')}}" class="itemSquare"><div ng-click="pick(newcomb)" translate="'postPickerApp_newComb'" class="pickerItem addComb"></div></div><div ng-show="session.placeholder != false" title="{{localization.translate('postpicker_newplaceholder')}}" class="itemSquare"><div ng-click="pick(placeholder, null, $event)" translate="'postPickerApp_newRule'" class="pickerItem addMediaplan"></div></div></div><div ng-show="makeArray(favourites).length &gt; 0" translate="'postPickerApp_favorites'" class="shelfTitle"></div><div ng-show="makeArray(favourites).length &gt; 0" class="favouriteBlocks shelf"><div ng-repeat="fav in favourites" class="itemSquare"><div ng-click="pick(fav.lotId, 'lot')" type="lot" class="pickerItem postSelectorItem"><div ng-click="unFavourite(fav.id, $event)" class="unFavourite">&#9733;</div><div type="lot" id="fav.lotId" class="itemPreview"></div></div></div></div><div translate="'postPickerApp_myPosts'" ng-show="makeArray(posts).length &gt; 0" class="shelfTitle"></div><div class="postBlocks shelf"><div ng-repeat="iterator in makeArray(posts) | filter:pickerFilter | reverse" class="itemSquare"><div ng-click="pick(iterator.id, 'post')" type="post" class="pickerItem postSelectorItem"><div type="post" id="iterator.id" class="itemPreview"></div></div></div></div></div></div>