<div class="dragHandler topBar noSettings black"><process-bar></process-bar><div class="appControls"></div><div ng-click="stepBack()" ng-if="canGoBack()" class="backButton"></div><div ng-if="currentStep.translateTitle" translate="currentStep.translateTitle" class="title"></div><div ng-if="!currentStep.translateTitle" class="title">{{currentStep.title}}</div><div class="centerButtons"><div translate="'marketApp_tabSwitch_repost'" ng-click="setMode('repost')" ng-class="{'active':currentStep.mode=='repost'}" class="tabSwitch"></div><div translate="'marketApp_tabSwitch_post'" ng-click="setMode('post')" ng-class="{'active':currentStep.mode=='post'}" class="tabSwitch"></div></div></div><div no-effects="no-effects" dropable-item="{'type':'marketApp'}" antiscroll="antiscroll" ng-style="{'background':sectionBackground()}" class="body droppableItem zoom_{{session.zoom}}"><div ng-if="currentStep.type == 'main' &amp;&amp; currentStep.mode == 'repost'" class="bigSlider"><div ng-click="slideRight()" class="arrow right"></div><div ng-click="slideLeft()" class="arrow left"></div><div ng-if="slides.length &gt; 1" class="paginatorCircle"><div ng-repeat="slide in slides" ng-class="{'active':$index == currentSlide}" ng-click="selectSlide($index)" class="page"></div></div></div><div ng-if="currentStep.src" ng-style="{'background':sectionPicture()}" class="sectionPicrute"></div><items-container></items-container></div><bread-crombs></bread-crombs>