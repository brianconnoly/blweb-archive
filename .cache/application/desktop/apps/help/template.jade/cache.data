<div class="dragHandler topBar noSettings black"><process-bar></process-bar><div class="appControls"></div><div ng-click="stepBack()" ng-if="canGoBack()" class="backButton"></div><div ng-if="currentStep.translateTitle" translate="currentStep.translateTitle" class="title"></div><div ng-if="!currentStep.translateTitle" class="title">{{currentStep.title}}</div></div><div class="body widthCrombs"><div class="helpLeft"><div ng-repeat="section in catalog" ng-class="{'active':currentSection == section}" class="section"><div ng-click="section.expanded = !section.expanded" class="sectionHead"><div ng-class="{'expanded':section.expanded}" class="expander"></div><div class="icon"><img src="{{section.icon}}"/></div><div translate="section.translateTitle" class="title"></div></div><div ng-style="{'height':getSectionHeight(section)}" class="sectionPages"><div ng-repeat="page in section.pages" ng-class="{'active':currentStep == page}" ng-click="selectSection(section, page)" translate="page.translateTitle" class="pageHead"></div></div></div></div><div ng-class="{'compact':compact}" class="helpRight"><div ng-if="currentStep.file" class="center"><object data="{{getHelpFile()}}" type="image/svg+xml" ng-class="{'visible':loaded}" class="helpFile"><embed src="{{getHelpFile()}}" type="image/svg+xml"/></object></div></div></div><bread-crombs></bread-crombs>