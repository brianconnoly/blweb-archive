<div class="courseList"><div class="courseTitle">Помощь Buzzlike</div><div ng-click="hideCourseList()" class="courseClose">Закрыть</div><div antiscroll="antiscroll" class="courseBlock"><div ng-repeat="course in courseList" ng-class="{'courseCur': course.show, 'courseLocked': course.locked}" class="courseItem"><div ng-click="toggleCourse(course)" class="courseRow"><div ng-class="{'notpassed':course.canceled, 'passed':course.passed}" class="status"></div><div class="name">{{course.name}}</div></div><div ng-bind-html="course.text" ng-show="course.show" class="courseText"></div><span ng-show="course.show" ng-click="openCourse(course)" class="openCourse">Пройти курс</span></div></div></div>