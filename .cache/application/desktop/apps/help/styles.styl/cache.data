.appContainer.help {
  background: #f2f2f2;
}
.appContainer.help .body {
  -webkit-box-shadow: none;
  box-shadow: none;
  border-top: 1px solid #ccc;
}
.appContainer.help .body .helpLeft {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  width: 205px;
  overflow-y: auto;
  border-right: 1px solid #ccc;
}
.appContainer.help .body .helpLeft .section.active .sectionHead,
.appContainer.help .body .helpLeft .section:hover .sectionHead {
  color: #75a843;
}
.appContainer.help .body .helpLeft .section .sectionHead {
  height: 34px;
  cursor: pointer;
}
.appContainer.help .body .helpLeft .section .sectionHead .expander {
  height: 34px;
  width: 24px;
  float: left;
  background: url("/resources/images/desktop/white/divider.svg") center center no-repeat;
  cursor: pointer;
  -webkit-transition: all 0.2s ease;
  -moz-transition: all 0.2s ease;
  -o-transition: all 0.2s ease;
  -ms-transition: all 0.2s ease;
  transition: all 0.2s ease;
  cursor: pointer;
}
.appContainer.help .body .helpLeft .section .sectionHead .expander.expanded {
  -webkit-transform: rotateZ(90deg) !important;
  -moz-transform: rotateZ(90deg) !important;
  -o-transform: rotateZ(90deg) !important;
  -ms-transform: rotateZ(90deg) !important;
  transform: rotateZ(90deg) !important;
}
.appContainer.help .body .helpLeft .section .sectionHead .icon {
  width: 16px;
  height: 34px;
  float: left;
  cursor: pointer;
}
.appContainer.help .body .helpLeft .section .sectionHead .icon img {
  width: 30px;
  height: 30px;
  margin-left: -7px;
  margin-top: 2px;
}
.appContainer.help .body .helpLeft .section .sectionHead .title {
  height: 34px;
  line-height: 34px;
  font-size: 12px;
  padding-left: 10px;
  cursor: pointer;
  float: left;
}
.appContainer.help .body .helpLeft .section .sectionPages {
  width: 100%;
  overflow: hidden;
  -webkit-transition: height 0.2s ease;
  -moz-transition: height 0.2s ease;
  -o-transition: height 0.2s ease;
  -ms-transition: height 0.2s ease;
  transition: height 0.2s ease;
}
.appContainer.help .body .helpLeft .section .sectionPages .pageHead {
  height: 34px;
  line-height: 34px;
  font-size: 12px;
  padding-left: 60px;
  cursor: pointer;
}
.appContainer.help .body .helpLeft .section .sectionPages .pageHead:hover {
  color: #75a843;
}
.appContainer.help .body .helpLeft .section .sectionPages .pageHead.active {
  background: #e1e1e1;
  color: #000 !important;
  cursor: default;
}
.appContainer.help .body .helpRight {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 206px;
  right: 0;
  overflow-y: auto;
  border-left: 1px solid #e6e6e6;
}
.appContainer.help .body .helpRight.compact .center {
  width: 100%;
  margin-left: 0;
  left: 0;
}
.appContainer.help .body .helpRight.compact .helpFile {
  width: 100%;
}
.appContainer.help .body .helpRight .center {
  position: absolute;
  width: 690px;
  left: 50%;
  margin-left: -345px;
  overflow-y: auto;
  overflow-x: hidden;
}
.appContainer.help .body .helpRight .helpFile {
  position: relative;
  display: block;
  width: 690px;
  -webkit-transition: opacity 0.2s ease;
  -moz-transition: opacity 0.2s ease;
  -o-transition: opacity 0.2s ease;
  -ms-transition: opacity 0.2s ease;
  transition: opacity 0.2s ease;
  margin: 0 auto;
  pointer-events: none;
}
